<%= form_for(@order) do |f| %>
  <p>
    <%= f.label :first_name %><br>
      <%= f.text_field :last_name %>
  </p>
  <p>
    <%= f.label :email %><br>
      <%= f.email_field :email %>
  </p>
  <p>
    <%= f.label :address %><br>
      <%= f.text_field :address %>
  </p>
  <p>
    <%= f.submit %>
  </p>
  <% end %>